<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//Mybatis.org/DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.edu.zjut.dao.ICustomerDAO">
    <insert id="save" parameterType="cn.edu.zjut.po.Customer">
            Insert into customer (customerID,account,password,name,sex,birthday,phone,email,address,zipcode,fax)
            values(#{customerID},#{account},#{password},#{name},#{sex},#{birthday},#{phone},#{email},#{address},#{zipcode},#{fax})
    </insert>

</mapper>